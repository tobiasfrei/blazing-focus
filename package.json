{
  "name": "ketchup-webpack-starterkit",
  "version": "1.0.1",
  "description": "Ketchup is a webpack boilerplate for developing static HTML pages with Nunjucks templates, ES201X transpiler and Sass precompiling with SVG/PNG sprites.",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --progress --mode development --config  webpack.config.dev.js --watch",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "lint": "lint-staged",
    "lint-all": "eslint \"src/**/*.js\" --fix",
    "pre-commit": "lint-staged"
  },
  "email": "mail@tobiasfrei.ch",
  "author": "Tobias Frei, Christian Sany",
  "license": "MIT",
  "homepage": "http://tobiasfrei.ch",
  "keywords": [
    "webpack4",
    "boilerplate",
    "Sass",
    "Babel",
    "SVG Sprite"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.4",
    "bourbon": "^5.1.0",
    "browser-sync": "^2.26.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "csscomb": "4.2.0",
    "es6-promise": "^4.1.1",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "lodash.merge": "^4.6.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^7.0.0",
    "nunjucks-html-loader": "^1.1.0",
    "nunjucks-loader": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.3",
    "svg-inline-loader": "^0.8.0",
    "svg-spritemap-webpack-plugin": "^2.7.0",
    "vue-loader": "^15.4.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.4",
    "webpack-modernizr-loader": "^4.0.1",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@unic/composite-observer": "0.0.10",
    "gsap": "^2.0.2",
    "mdn-polyfills": "^5.9.0",
    "modernizr": "^3.6.0",
    "normalize.css": "^8.0.0"
  },
  "lint-staged": {
    "*.scss": [
      "csscomb --tty-mode",
      "git add"
    ],
    "*.js": [
      "eslint --fix \"{,**,**/**}*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github.com/tobiasfrei"
  },
  "browserslist": [
    "last 2 versions",
    "ie > 10",
    "iOS > 9"
  ]
}
